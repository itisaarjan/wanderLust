<%layout("/layouts/boilerplate")%>
<body>
    <form action="/listings/new" method="post">
        <input type="text" name="title" placeholder="Enter title of the property">
        <input type="textarea" name="description" placeholder="Enter description of the property">
        <input type="text" name="imageUrl" placeholder="Enter the image url of the property image">
        <input type="number" name="price" placeholder="Enter price">
        <input type="text" name="location" placeholder="Enter location of the property">
        <input type="text" name="country" placeholder="Enter country located">
        <button type="submit">Submit</button>
    </form>
</body>
